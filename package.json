{
  "name": "react-template",
  "version": "1.0.0",
  "repository": "https://github.com/JakeDluhy/react-template",
  "description": "",
  "main": "tools/webpack.config.js",
  "scripts": {
    "serve": "bnr serve",
    "test": "bnr test",
    "build-dll:web": "bnr build-dll:web",
    "build": "webpack --config ./tools/webpack.config.js --profile --colors --env.sourcemap=source-map",
    "--": "",
    "test:unit": "bnr test:unit",
    "test:integration": "bnr test:integration",
    "test:coverage": "bnr test:coverage"
  },
  "betterScripts": {
    "serve": {
      "command": "node tools/parts/server.js",
      "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "web",
        "TARGET": "web"
      }
    },
    "test": {
      "command": "mocha test/integrationHelper.js test/unit test/integration --compilers js:babel-core/register --require ./test/testHelper.js --require ./test/assetsNullCompiler.js",
      "env": {
        "NODE_ENV": "test",
        "NO_LOGGING": "true",
        "BABEL_ENV": "test"
      }
    },
    "build-dll:web": {
      "command": "webpack --config ./tools/webpack.config.js --profile --colors --env.sourcemap=source-map",
      "env": {
        "BABEL_ENV": "web",
        "TARGET": "dll"
      }
    },
    "build-web": {
      "command": "webpack --config ./tools/webpack.config.js --profile --colors --env.sourcemap=source-map",
      "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "web",
        "TARGET": "web"
      }
    },
    "test:unit": {
      "command": "mocha test/unit --compilers js:babel-core/register --require ./test/testHelper.js --require ./test/assetsNullCompiler.js",
      "env": {
        "NODE_ENV": "test",
        "NO_LOGGING": "true",
        "BABEL_ENV": "test"
      }
    },
    "test:integration": {
      "command": "mocha test/integrationHelper.js test/integration --compilers js:babel-core/register --require ./test/testHelper.js --require ./test/assetsNullCompiler.js",
      "env": {
        "NODE_ENV": "test",
        "NO_LOGGING": "true",
        "BABEL_ENV": "test"
      }
    },
    "test:coverage": {
      "command": "istanbul cover node_modules/mocha/bin/_mocha --compilers js:babel-core/register --require ./test/testHelper.js --require ./test/assetsNullCompiler test/integrationHelper.js test/unit test/integration",
      "env": {
        "NODE_ENV": "test",
        "NO_LOGGING": "true",
        "BABEL_ENV": "test"
      }
    }
  },
  "author": "",
  "license": "ISC",
